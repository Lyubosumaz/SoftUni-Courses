<h1>Projects</h1>
<form class="search" action="/projects/search" method="GET">
    <label>Search: </label>
    <input type="text" name="search">
    <input type="submit" value="Search">
</form>
<div class="projects">
    {{#each data}}
    <div class="project">
        <h2>{{this.name}}</h2>
        {{#if this.team}}
        <h3>Team: {{this.team.name}}</h3>
        {{else}}
        <h3>Team: No Team assigned</h3>
        {{/if}}
        <p>Description: {{this.description}}</p>
    </div>
    {{/each}}
</div>